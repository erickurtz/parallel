#include <stdlib.h>
#include <stdio.h>
#include <assert.h>
#include <math.h>

int n, m;
double result;
double ** a;


void init(int argc, char* argv[]) {
  assert(argc == 3);
  n = atoi(argv[1]);
  m = atoi(argv[2]);
  result = 0.0;
  a = (double **)malloc(n*sizeof(*a));
  for (int i=0; i<n; i++) {
    a[i] = (double *)malloc(m*sizeof(*a[i]));
    for (int j=0; j<m; j++)
      a[i][j] = i*2.0 + j*1.0;
  }
}

/* 
 * Note that the L2-norm is calculated on a column.
 */
void update() {
  double norm;
  
  for (int i=0; i<m; i++) {
    norm = 0.0;
    for (int j=0; j<n; j++)
      norm = norm + a[i][j]*a[i][j];
    norm = sqrt(norm);
    result = result + norm;
  }

}

int main (int argc, char* argv[]) {
  init(argc, argv);
  update();
  printf("%f\n", result);
  return 0;
}
